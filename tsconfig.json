{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ES2022",
		"moduleResolution": "nodenext",
		"moduleDetection": "auto",
		"esModuleInterop": true,
		"outDir": "exports",
		"declaration": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"useDefineForClassFields": false,
		"resolveJsonModule": true
	},
	"include": [
		"./**/*"
, ".vscode/elements.ts"	]
}
